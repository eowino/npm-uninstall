{
	"name": "npm-uninstall",
	"version": "0.0.1",
	"description": "uninstall your npm dependencies",
	"author": "Evans Owino",
	"homepage": "https://github.com/eowino/npm-uninstall",
	"license": "MIT",
	"keywords": [
		"uninstall",
		"dependencies",
		"clean",
		"delete"
	],
	"main": "dist/index.js",
	"files": [
		"dist/**/*",
		"bin/**/*"
	],
	"engines": {
		"node": ">=8.10.0"
	},
	"bin": {
		"npm-uninstall": "./bin/index.js"
	},
	"scripts": {
		"start": "tsc --watch",
		"build": "npm run clean && tsc",
		"clean": "rm -rf dist",
		"debug": "node --inspect-brk dist/index.js",
		"debug:clean": "npm run build && npm run debug",
		"run": "npm run build && node dist/index.js",
		"format": "eslint . --fix '**/*.{js,ts}'",
		"format:check": "eslint . '**/*.{js,ts}'",
		"precommit": "lint-staged",
		"prepare": "npm run build",
		"postpublish": "git push --tags"
	},
	"lint-staged": {
		"*.{json,md}": [
			"prettier --write",
			"git add"
		],
		"*.{ts,js}": [
			"eslint --fix",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run precommit"
		}
	},
	"devDependencies": {
		"@types/inquirer": "^6.5.0",
		"@types/node": "^13.15.0",
		"@typescript-eslint/eslint-plugin": "^2.1.0",
		"@typescript-eslint/parser": "^2.1.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-prettier": "^3.1.1",
		"husky": "^3.0.8",
		"lint-staged": "^9.4.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"typescript": "^3.7.5"
	},
	"dependencies": {
		"inquirer": "^7.0.0"
	}
}
